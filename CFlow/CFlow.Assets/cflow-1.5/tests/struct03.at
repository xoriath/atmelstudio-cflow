AT_SETUP([K&R C function returning a struct])
AT_KEYWORDS([struct struct03 ret-struct])

CFLOW_CHECK(
struct s *
foo(arg)
	int arg;
{
	bar();
} 
int
main(argc, argv)
	int argc;
	char **argv;
{
	foo();
}
],
[main() <int main (argc, argv) at prog:8>:
    foo() <struct s *foo (arg) at prog:2>:
        bar()
])

AT_CLEANUP
